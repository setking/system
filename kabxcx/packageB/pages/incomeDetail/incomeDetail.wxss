/* pages/incomeDetail/incomeDetail.wxss */
.person-list-main{
  width: 90%;
  height: 38px;
  margin: 0 auto;
  margin-top: 20px;
}
.person-list-left{
  width: 70%;
  height: 38px;
  float: left;
}
.person-img{
  width: 30px;
  height: 30px;
  float: left;
}
.person-title{
  color: #433D3D;
  font-size: 11px;
  letter-spacing: 0.48px;
  margin-left: 87rpx;
}
.person-time{
  color: #9b9898;
  font-size: 11px;
  letter-spacing: 0.48px;
  margin-left: 87rpx;
}
.person-list-right{
  width: 30%;
  height: 38px;
  float: left;
  color: #B1ADAD;
  letter-spacing: 0.74px;
  font-size: 17px;
  text-align: right;
  line-height: 38px;
}
.no-card{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.no-card image{
  width: 178px;
  height: 176px;
  margin: 40px auto;
  display: block;
}
.no-text{
  text-align: center;
  font-size: 17px;
  color: #d7d7d7;
  letter-spacing: 1px;
}